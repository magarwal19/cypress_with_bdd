{
  "name": "react-redux-realworld-example-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "cross-env": "^5.1.4",
    "cucumber-html-reporter": "^5.2.0",
    "cypress": "^4.5.0",
    "cypress-cucumber-preprocessor": "^2.3.1",
    "react-scripts": "3.4.1"
  },
  "dependencies": {
    "history": "^4.6.3",
    "marked": "^0.3.6",
    "prop-types": "^15.5.10",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.1",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env PORT=4100 react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env PORT=4100 react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "delete:reports": "if exist  \".\\cypress\\results\" rmdir .\\cypress\\results /q /s",
    "pretestE2E": "npm run delete:reports",
    "testE2E": "./node_modules/.bin/cypress run --headed",
    "generateHTMLReport": "node index.js",
    "getFeatureFiles": "hiptest-publisher -c hiptest-publisher.conf --only=features"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/integration/featureFiles/stepdefinition/",
    "commonPath": "cypress/integration/featureFiles/stepdefinition/common/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "./cypress/results/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
